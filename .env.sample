# SecureApp Environment Configuration
# Copy this file to .env to customise your local setup

# By default a SQLite database stored in secureapp.db is used.
# To use PostgreSQL set DATABASE_URL to something like:
#   postgresql://username:password@localhost:5432/database_name
DATABASE_URL=sqlite:///secureapp.db

# Generate a secure random value for production
SESSION_SECRET=dev-secret

# OAuth configuration (optional)
REPL_ID=

