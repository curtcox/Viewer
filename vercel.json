{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/index.py": {
      "includeFiles": [
        "cids/**",
        "reference_templates/readonly.boot.cid"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "SESSION_SECRET": "@session_secret",
    "READ_ONLY": "true",
    "GIT_SHA": "@git_sha"
  }
}
