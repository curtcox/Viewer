      "jsonplaceholder_error.html": "reference_templates/gateways/templates/jsonplaceholder_error.html"
    }
  },
  "man": {
    "request_transform_cid": "reference_templates/gateways/transforms/man_request.py",
    "response_transform_cid": "reference_templates/gateways/transforms/man_response.py",
    "description": "HTML formatted man pages with command links",
    "templates": {
      "man_page.html": "reference_templates/gateways/templates/man_page.html",
      "man_error.html": "reference_templates/gateways/templates/man_error.html"
    }
  },
  "tldr": {
    "request_transform_cid": "reference_templates/gateways/transforms/tldr_request.py",
    "response_transform_cid": "reference_templates/gateways/transforms/tldr_response.py",
    "description": "HTML formatted tldr pages with command links",
    "templates": {
      "tldr_page.html": "reference_templates/gateways/templates/tldr_page.html",
      "tldr_error.html": "reference_templates/gateways/templates/tldr_error.html"
    }
  },
  "hrx": {
    "request_transform_cid": "reference_templates/gateways/transforms/hrx_request.py",
    "response_transform_cid": "reference_templates/gateways/transforms/hrx_response.py",
    "description": "HRX archive viewer with markdown and code highlighting",
    "templates": {
      "hrx_error.html": "reference_templates/gateways/templates/hrx_error.html",
      "hrx_directory.html": "reference_templates/gateways/templates/hrx_directory.html",
      "hrx_markdown.html": "reference_templates/gateways/templates/hrx_markdown.html",
      "hrx_text.html": "reference_templates/gateways/templates/hrx_text.html"
