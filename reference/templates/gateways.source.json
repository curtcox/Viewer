  "json_api": {
    "request_transform_cid": "reference/templates/gateways/transforms/json_api_request.py",
    "response_transform_cid": "reference/templates/gateways/transforms/json_api_response.py",
    "description": "Generalized JSON API gateway with link detection",
    "templates": {
      "json_api_data.html": "reference/templates/gateways/templates/json_api_data.html"
    },
    "link_detection": {
      "full_url": {
        "enabled": true,
        "base_url_strip": "https://jsonplaceholder.typicode.com",
        "gateway_prefix": "/gateway/json_api"
      },
      "partial_url": {
        "enabled": true,
        "key_patterns": [
          "url",
          "*_url",
          "*_path",
          "href"
        ],
        "gateway_prefix": "/gateway/json_api"
      },
      "id_reference": {
        "enabled": true,
        "patterns": {
          "userId": "/gateway/json_api/users/{id}",
          "postId": "/gateway/json_api/posts/{id}",
          "albumId": "/gateway/json_api/albums/{id}"
        }
      }
    }
  },
  "github": {
    "request_transform_cid": "reference/templates/gateways/transforms/json_api_request.py",
    "response_transform_cid": "reference/templates/gateways/transforms/json_api_response.py",
    "description": "GitHub API via JSON API gateway",
    "external_api": "https://api.github.com",
    "templates": {
      "json_api_data.html": "reference/templates/gateways/templates/json_api_data.html"
    },
    "link_detection": {
      "full_url": {
        "enabled": true,
        "base_url_strip": "https://api.github.com",
        "gateway_prefix": "/gateway/github"
      },
      "partial_url": {
        "enabled": true,
        "key_patterns": [
          "url",
          "*_url",
          "*_path",
          "href"
        ],
        "gateway_prefix": "/gateway/github"
      },
      "id_reference": {
        "enabled": true,
        "patterns": {
          "login": "/gateway/github/users/{id}"
        }
      },
      "composite_reference": {
        "enabled": true,
        "patterns": {
          "number": {
            "context_regex": "repos/(?P<owner>[^/]+)/(?P<repo>[^/]+)/(issues|pulls)$",
            "url_template": "/gateway/github/repos/{owner}/{repo}/issues/{id}"
          }
        }
      }
    }
  },
